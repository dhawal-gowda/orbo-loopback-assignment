{
  "name": "MemberAuth",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "signUp"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW",
      "property": "makeAdmin"
    }
  ],
  "methods": {}
}
